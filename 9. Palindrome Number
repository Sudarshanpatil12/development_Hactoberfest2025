class Solution {
    public boolean isPalindrome(int x) {
        
        long rev = 0;
        int y = x;

        if ( x < 0){
            return false;
        }
        while(y!= 0){
            long remain = y % 10;
            rev = rev * 10 + remain;
            y = y / 10;
        }
        if(x != rev){
            return false;
        }
        return true;
    }
}
